#include<stdio.h>
#include<stdlib.h>
int missNumber(int *arr, int *arr_size){
int sum=0, n=arr_size+1;
for(int i=0;i<arr_size;i++){
sum=sum+arr[i];
}
return(n*(n+1))/2-sum;
}
int main(){
int i;
int arr{}={1.5.3.4};
int n1=sizeof(arr)/{sizeofarr[0]);
printf("The given array is:");
for(i=0;i<n1;i++){
printf("%d",arr[i]);
}
printf("\n");
printf("missing number is:%d\n");
missNumber(arr,n1);
return 0;
}
